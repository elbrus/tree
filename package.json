{
	"name": "rc-tree",
	"version": "1.7.1",
	"description": "tree ui component for react",
	"keywords": [
		"react",
		"react-component",
		"react-tree",
		"tree"
	],
	"main": "lib/index",
	"module": "./es/index",
	"files": [
		"assets/*.css",
		"assets/*.png",
		"assets/*.gif",
		"dist",
		"es",
		"lib"
	],
	"entry": {
		"rc-tree": [
			"./assets/index.less",
			"./src/index.js"
		]
	},
	"homepage": "http://github.com/react-component/tree",
	"author": "hualei5280@gmail.com",
	"repository": {
		"type": "git",
		"url": "git@github.com:react-component/tree.git"
	},
	"bugs": {
		"url": "http://github.com/react-component/tree/issues"
	},
	"license": "MIT",
	"config": {
		"port": 8018
	},
	"scripts": {
		"build": "rc-tools run build",
		"dist": "rc-tools run dist",
		"compile": "rc-tools run compile --babel-runtime",
		"gh-pages": "rc-tools run gh-pages",
		"start": "rc-tools run server",
		"pub": "rc-tools run pub --babel-runtime",
		"lint": "rc-tools run lint",
		"test": "jest",
		"coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*"
		],
		"transform": {
			"\\.jsx?$": "./node_modules/rc-tools/scripts/jestPreprocessor.js"
		}
	},
	"devDependencies": {
		"babel-preset-es2015-ie": "6.7.0",
		"coveralls": "^2.11.15",
		"css-animation": "^1.2.0",
		"enzyme": "^2.6.0",
		"enzyme-to-json": "^1.4.5",
		"es6-promise": "^3.1.2",
		"jest": "^18.0.0",
		"pre-commit": "1.x",
		"rc-dialog": "^6.5.9",
		"rc-tools": "^6.x",
		"rc-tooltip": "https://github.com/elbrus/tooltip/tarball/a923a17cf78b6c1606551dc134da4b5f2c3a9a93",
		"rc-trigger": "https://github.com/elbrus/trigger/tarball/052aa588e1f860b8d03bde06fb0396d601177019",
		"react": "15.x",
		"react-dom": "15.x",
		"react-test-renderer": "^15.5.4"
	},
	"dependencies": {
		"babel-runtime": "^6.23.0",
		"classnames": "^2.2.1",
		"prop-types": "^15.5.10",
		"rc-animate": "^2.4.1",
		"rc-util": "^4.0.4",
		"warning": "^3.0.0"
	}
}
